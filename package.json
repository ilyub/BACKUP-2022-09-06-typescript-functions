{
  "name": "@skylib/functions",
  "version": "0.1.21",
  "private": false,
  "description": "Typescript functions",
  "keywords": [
    "function",
    "typescript"
  ],
  "homepage": "https://github.com/ilyub/typescript-functions",
  "bugs": {
    "url": "https://github.com/ilyub/typescript-functions/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/typescript-functions.git"
  },
  "license": "ISC",
  "author": "Ilya Lyubinskiy <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "build-doc": "typedoc src",
    "build-es": "ttsc --project tsconfig-build-es.json",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "test": "jest",
    "test-fast": "jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,prod --reject @types/node",
    "update-package-check-peer": "ncu --dep peer --minimal --reject @types/node",
    "update-package-check-skylib": "ncu --dep peer --filter @skylib/*",
    "update-package-force": "ncu --dep dev,prod --reject @types/node --upgrade",
    "update-package-force-peer": "ncu --dep peer --minimal --reject @types/node --upgrade",
    "update-package-force-skylib": "ncu --dep peer --filter @skylib/* --upgrade"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@jest/test-sequencer": "^27.5.1",
    "@skylib/config": "^0.1.38",
    "@skylib/eslint-plugin": "^0.1.10",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-boundaries": "^2.8.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsdoc": "^37.9.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-more": "^1.0.5",
    "eslint-plugin-no-type-assertion": "^1.1.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-export-all": "^1.2.1",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-xss": "^0.1.11",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-environment-node": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "npm-check-updates": "^12.4.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-transform-import-path-rewrite": "^0.3.0",
    "ttypescript": "^1.5.13",
    "typedoc": "^0.22.12",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "^4.5.5",
    "vue-eslint-parser": "^8.2.0"
  },
  "peerDependencies": {
    "@sinonjs/fake-timers": "^9.0.0",
    "@skylib/typescript-types": "^0.1.1",
    "@types/jquery": "^3.5.8",
    "@types/lodash": "^4.14.176",
    "@types/lodash-es": "^4.17.5",
    "@types/sinonjs__fake-timers": "^8.1.0",
    "jest-extended": "^2.0.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "ts-toolbelt": "^9.6.0",
    "tslib": "^2.3.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
