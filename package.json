{
  "name": "@skylib/functions",
  "version": "2.4.0",
  "private": false,
  "description": "Typescript functions",
  "keywords": [
    "function",
    "typescript"
  ],
  "homepage": "https://github.com/ilyub/typescript-functions",
  "bugs": {
    "url": "https://github.com/ilyub/typescript-functions/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/typescript-functions.git"
  },
  "license": "ISC",
  "author": "Ilia Liubinskii <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "dist/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "build-doc": "typedoc src",
    "build-es": "tsc --project tsconfig-build-es.json",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "test": "jest",
    "test-fast": "jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,peer,prod",
    "update-package-force": "ncu --dep dev,peer,prod --upgrade"
  },
  "devDependencies": {
    "@skylib/config": "^1.15.0",
    "@skylib/eslint-plugin": "^0.1.28",
    "@skylib/typescript-types": "^1.0.0"
  },
  "peerDependencies": {
    "@sinonjs/fake-timers": ">=9",
    "@skylib/lodash-commonjs-es": ">=0",
    "@types/sinonjs__fake-timers": ">=8",
    "jest-extended": ">=2",
    "reflect-metadata": ">=0",
    "ts-toolbelt": ">=9"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
